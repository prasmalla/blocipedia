<tr>
  <td><%= wiki.title %></td>
  <td>
    <%= link_to edit_wiki_path(wiki) do %>
      <i class="glyphicon glyphicon-edit"></i> Edit
    <% end %>
  </td>
  <td>
    <%= link_to wiki, method: :delete, data: { confirm: 'Are you sure you want to delete this wiki?' } do %>
      <i class="glyphicon glyphicon-trash"></i> Delete
    <% end %>
  </td>
</tr>